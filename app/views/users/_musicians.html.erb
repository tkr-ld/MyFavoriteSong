<% if musicians.any? %>
  <ul class="media-list">
    <% musicians.each do |musician| %>
      <li class="media">
        <div class="media-body">
          <div>
            <p><%= link_to musician.name, musician_path(musician) %></p>
          </div>
        </div>
      </li>
    <% end %>
  </ul>
<% end %>